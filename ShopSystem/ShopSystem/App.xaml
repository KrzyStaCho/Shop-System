             xmlns:local="clr-namespace:ShopSystem"
             StartupUri="MVVM/View/MainWindowView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MVVM/View/Style/UIColors.xaml"/>
